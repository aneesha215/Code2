<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ClassController</servlet-name>
  	<display-name>ClassController</display-name>
  	<description></description>
  	<servlet-class>com.controller.ClassController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SubjectController</servlet-name>
  	<display-name>SubjectController</display-name>
  	<description></description>
  	<servlet-class>com.controller.SubjectController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentController</servlet-name>
  	<display-name>StudentController</display-name>
  	<description></description>
  	<servlet-class>com.controller.StudentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TeacherController</servlet-name>
  	<display-name>TeacherController</display-name>
  	<description></description>
  	<servlet-class>com.controller.TeacherController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MapClassSubjectController</servlet-name>
  	<display-name>MapClassSubjectController</display-name>
  	<description></description>
  	<servlet-class>com.controller.MapClassSubjectController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddStudentClassController</servlet-name>
  	<display-name>AddStudentClassController</display-name>
  	<description></description>
  	<servlet-class>com.controller.AddStudentClassController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReportController</servlet-name>
  	<display-name>ReportController</display-name>
  	<description></description>
  	<servlet-class>com.controller.ReportController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MapTSCController</servlet-name>
  	<display-name>MapTSCController</display-name>
  	<description></description>
  	<servlet-class>com.controller.MapTSCController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ClassController</servlet-name>
  	<url-pattern>/Class</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SubjectController</servlet-name>
  	<url-pattern>/Subject</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentController</servlet-name>
  	<url-pattern>/Student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TeacherController</servlet-name>
  	<url-pattern>/Teacher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MapClassSubjectController</servlet-name>
  	<url-pattern>/MapClassSubject</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddStudentClassController</servlet-name>
  	<url-pattern>/AddStudentClass</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReportController</servlet-name>
  	<url-pattern>/Report</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MapTSCController</servlet-name>
  	<url-pattern>/MapTSC</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/Login</url-pattern>
  </servlet-mapping>
</web-app>
